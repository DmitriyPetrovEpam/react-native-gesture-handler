{
  "extends": "satya164",
  "settings": {
    "react": { "version": "16" },
    "import/resolver": {
      "babel-module": {}
    }
  },
  "env": { "browser": true, "node": true, "jest/globals": true },
  "plugins": ["jest"],
  "rules": {
    "import/named": "off",
    "react/sort-comp": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error",
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": "error"
  }
}
